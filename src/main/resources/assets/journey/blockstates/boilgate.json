{
    "multipart": [
      {
        "apply": {
          "model": "journey:pane/boilgate/post"
        }
      },
      {
        "when": {
          "north": true
        },
        "apply": {
          "model": "journey:pane/boilgate/side"
        }
      },
      {
        "when": {
          "east": true
        },
        "apply": {
          "model": "journey:pane/boilgate/side",
          "y": 90
        }
      },
      {
        "when": {
          "south": true
        },
        "apply": {
          "model": "journey:pane/boilgate/side_alt"
        }
      },
      {
        "when": {
          "west": true
        },
        "apply": {
          "model": "journey:pane/boilgate/side_alt",
          "y": 90
        }
      },
      {
        "when": {
          "north": false
        },
        "apply": {
          "model": "journey:pane/boilgate/noside"
        }
      },
      {
        "when": {
          "east": false
        },
        "apply": {
          "model": "journey:pane/boilgate/noside_alt"
        }
      },
      {
        "when": {
          "south": false
        },
        "apply": {
          "model": "journey:pane/boilgate/noside_alt",
          "y": 90
        }
      },
      {
        "when": {
          "west": false
        },
        "apply": {
          "model": "journey:pane/boilgate/noside",
          "y": 270
        }
      }
    ]
}
