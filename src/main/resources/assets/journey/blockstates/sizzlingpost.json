{
    
	"multipart": [
        
	{   
		"apply": { "model": "journey:sizzlingpost" }
	},
        
	{   
		"when": { "north": "true" },
            
		"apply": { "model": "journey:sizzlingpost_side", 
		"uvlock": true }

	},
       
	{   
		"when": { "east": "true" },
            
		"apply": { "model": "journey:sizzlingpost_side", 
		"y": 90, "uvlock": true }
        
	},
        
	{   
		"when": { "south": "true" },
            
		"apply": { "model": "journey:sizzlingpost_side",
		"y": 180, "uvlock": true }
       
	},
        
	{   
		"when": { "west": "true" },
            
		"apply": { "model": "journey:sizzlingpost_side", 
		"y": 270, "uvlock": true }
        	
	}
    
	]

}