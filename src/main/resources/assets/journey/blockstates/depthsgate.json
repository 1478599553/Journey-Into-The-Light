{
  "multipart": [
    {
      "apply": {
        "model": "journey:pane/depthsgate/post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "journey:pane/depthsgate/side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "journey:pane/depthsgate/side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "journey:pane/depthsgate/side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "journey:pane/depthsgate/side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "journey:pane/depthsgate/noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "journey:pane/depthsgate/noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "journey:pane/depthsgate/noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "journey:pane/depthsgate/noside",
        "y": 270
      }
    }
  ]
}