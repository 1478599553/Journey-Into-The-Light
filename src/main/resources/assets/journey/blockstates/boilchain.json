{
    "multipart": [
        {
            "apply": {
                "model": "journey:pane/boilchain/post"
            }
        },
        {
            "when": {
                "north": true
            },
            "apply": {
                "model": "journey:pane/boilchain/side"
            }
        },
        {
            "when": {
                "east": true
            },
            "apply": {
                "model": "journey:pane/boilchain/side",
                "y": 90
            }
        },
        {
            "when": {
                "south": true
            },
            "apply": {
                "model": "journey:pane/boilchain/side_alt"
            }
        },
        {
            "when": {
                "west": true
            },
            "apply": {
                "model": "journey:pane/boilchain/side_alt",
                "y": 90
            }
        },
        {
            "when": {
                "north": false
            },
            "apply": {
                "model": "journey:pane/boilchain/noside"
            }
        },
        {
            "when": {
                "east": false
            },
            "apply": {
                "model": "journey:pane/boilchain/noside_alt"
            }
        },
        {
            "when": {
                "south": false
            },
            "apply": {
                "model": "journey:pane/boilchain/noside_alt",
                "y": 90
            }
        },
        {
            "when": {
                "west": false
            },
            "apply": {
                "model": "journey:pane/boilchain/noside",
                "y": 270
            }
        }
    ]
}
